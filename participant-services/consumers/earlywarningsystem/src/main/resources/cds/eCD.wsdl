<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://it.univaq.incipict.ems/eCD"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	name="eCD"
	targetNamespace="http://it.univaq.incipict.ems/eCD">

  <wsdl:types>
    <xsd:schema targetNamespace="http://it.univaq.incipict.ems/eCD">
    	<xsd:element name="prepareRequest" type="xsd:string"/>
    </xsd:schema>
  </wsdl:types>

  <wsdl:message name="prepareRequest">
    <wsdl:part element="tns:prepareRequest" name="request"/>
  </wsdl:message>

  <wsdl:portType name="eCDPT">
  	<wsdl:operation name="Prepare">
      <wsdl:input message="tns:prepareRequest"/>
    </wsdl:operation>
  </wsdl:portType>

  <wsdl:binding name="eCDBinding" type="tns:eCDPT">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="Prepare">
      <soap:operation soapAction=""/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
    </wsdl:operation>
  </wsdl:binding>

  <wsdl:service name="eCDService">
    <wsdl:port binding="tns:eCDBinding" name="eCDPort">
      <soap:address location="http://localhost:8080/ode/processes/eCD"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
